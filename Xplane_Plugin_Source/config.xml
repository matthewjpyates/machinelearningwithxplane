<?xml version="1.0">
<plugin>
	<settings name="xmlPluginTest" recordRate="1.0" stepRate=".25" 
		reloadEveryRun="false" reloadEveryChange="false" debug="false">
	</settings>
	<measures>
		<measure>some dataref 1</measure>
		<measure>some dataref 2</measure>
		<measure>some dataref 3</measure>
	</measures>
	<runresets>
		<runreset dataref="some dataref 4" value=".25"> </runreset>
		<runreset dataref="some dataref 5" value=".75"> </runreset>
	</runresets>
	<recordresets>
		<recordreset dataref="some dataref 4" value=".25"> </recordreset>
		<recordreset dataref="some dataref 5" value=".75"> </recordreset>
	</recordresets>
	<stepresets>
		<stepreset dataref="some dataref 4" value=".25"> </stepreset>
		<stepreset dataref="some dataref 5" value=".75"> </stepreset>
	</stepresets>
	<samples>
		<sample dataref="the data ref to be changed">
			<listoptions>
				<option>.3</option>
				<option>.6</option>
				<option>.9</option>
			</listoptions>
		</sample>
	</samples>
	<learned>
		<learnedref dataref="dataref the has been learned">
			<listref>
				<dataref>dataref ref read from 1</dataref>
				<dataref>dataref ref read from 2</dataref>
			</listref>
			<listoptions>
				<option>.32</option>
				<option>.62</option>
				<option>.92</option>
			</listoptions>
			<listweights>
				<weight>.33</weight>
				<weight>.63</weight>
				<weight>.93</weight>
			</listweights>
		</learnedref>
	</learned>
</plugin>
